<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	
	<bean id="cource1" class="com.mypack.Cource">
		<property name="id" value="101"/>
		<property name="name" value="PHP"/>
	</bean>
	
	<bean id="userBean" class="com.mypack.User">
		<property name="roll" value="201"/>
		<property name="name" value="Imran Khan"/>
		<property name="cource" ref="cource1"/>
	</bean>
</beans>